<TextView android:layout_height="wrap_content" android:layout_alignParentRight="true" android:layout_width="wrap_content" android:id="@+id/item_prenotazione_tv_Data"></TextView>
<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentRight="true" android:id="@+id/item_prenotazione_tv_id_key" android:layout_alignParentLeft="true" android:visibility="invisible">
	</TextView></RelativeLayout>




